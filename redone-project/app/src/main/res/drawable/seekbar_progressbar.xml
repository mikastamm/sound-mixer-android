<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@+id/seekbar_background">
        <shape android:shape="rectangle">
            <stroke android:width="5dp" android:color="#00ffffff"/>
            <corners
                android:radius="15dp" />
            <gradient
                android:angle="270"
                android:startColor="#33000000"
                android:centerColor="#11000000"
                android:endColor="#11000000"
                android:centerY="0.2"
                android:type="linear"
                />
        </shape>

    </item>
    <item android:id="@+id/seekbar_filled">
        <clip>
            <shape android:shape="rectangle" >
                <stroke android:width="5dp" android:color="#00ffffff"/>
                <corners android:radius="15dp" />
                <solid android:color="@color/colorAccent"/>
              <!--  <gradient
                    android:angle="270"
                    android:startColor="#d97425"
                    android:centerColor="#FFD54F"
                    android:endColor="#FFD54F"
                    android:centerY="0.2"
                    android:type="linear" />-->


            </shape>
        </clip>
    </item>


</layer-list>